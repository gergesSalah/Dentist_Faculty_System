@if (User.IsInRole("Doctor"))
{
    <section>
        <div class="row justify-content-end">
            <div class="col-lg-4">
                <form class="form-inline my-2 my-lg-0" asp-controller="Home" asp-action="Search">
                    <input class="form-control mr-sm-2" name="term" type="search" placeholder="search here...">
                    <button class="btn btn-outline-success " type="submit">Search</button>
                </form>
            </div>
        </div>
        @foreach (var group in Model)
        {
            <div class="card mb-4">
                <div class="card-header">
                    <h2 class="card-title">@group.Key</h2>
                </div>
                <div class="card-body">
                    <div class="row">
                        @foreach (var item in group)
                        {
                            <div class="card mb-3" style="max-width: 340px;margin:10px">
                                <div class="row no-gutters">
                                    <div class="col-md-4">
                                        <img src="data:image/*;base64,@Convert.ToBase64String(item.DiseasePicture)" class="card-img" alt="...">
                                    </div>
                                    <div class="col-md-8">
                                        <div class="card-body">
                                            <h5 class="card-title">@item.fname@item.lname</h5>
                                            <p class="card-text">@item.Dname</p>
                                            <p class="card-text"><small class="text-muted">@item.age</small></p>
                                            <a href="/Home/ConnectPatient/@item.CaseId" class="btn btn-secondary">Contact</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        }

    </section>
}

